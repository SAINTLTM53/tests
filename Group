local Players = game:GetService("Players")
local TeleportService = game:GetService("TeleportService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local LocalPlayer = Players.LocalPlayer
local Version = game.PlaceVersion

local safeParent = gethui and gethui() or game:GetService("CoreGui")

local gui = Instance.new("ScreenGui")
gui.Name = "UI_" .. tostring(math.random(1000, 9999))
gui.ResetOnSpawn = false
gui.DisplayOrder = 1000
gui.IgnoreGuiInset = true
gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
gui.Parent = safeParent

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 160, 0, 80)
frame.Position = UDim2.new(0.4, 0, 0.4, 0)
frame.BackgroundColor3 = Color3.new(0, 0, 0)
frame.Active = true
frame.Draggable = true
frame.Visible = true
frame.Parent = gui

local actionButton = Instance.new("TextButton")
actionButton.Size = UDim2.new(1, -20, 0, 40)
actionButton.Position = UDim2.new(0, 10, 0, 20)
actionButton.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
actionButton.TextColor3 = Color3.fromRGB(170, 0, 255)
actionButton.Font = Enum.Font.GothamBold
actionButton.TextSize = 14
actionButton.Text = "Loading..."
actionButton.Parent = frame

local circleBtn = Instance.new("TextButton")
circleBtn.Size = UDim2.new(0, 50, 0, 50)
circleBtn.Position = UDim2.new(0, 20, 0, 20)
circleBtn.BackgroundColor3 = Color3.fromRGB(170, 0, 255)
circleBtn.Text = "-"
circleBtn.TextColor3 = Color3.new(0, 0, 0)
circleBtn.Font = Enum.Font.GothamBold
circleBtn.TextSize = 24
circleBtn.Active = true
circleBtn.Draggable = true
circleBtn.Parent = gui

local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(1, 0)
corner.Parent = circleBtn

circleBtn.MouseButton1Click:Connect(function()
	frame.Visible = not frame.Visible
end)

if Version >= 2990 then
	actionButton.Text = "Join Dupe Server"
	actionButton.MouseButton1Click:Connect(function()
		TeleportService:Teleport(82028255140111, LocalPlayer)
		gui:Destroy()
	end)
elseif Version <= 10 then
	actionButton.Text = "Dupe Money"
	actionButton.MouseButton1Click:Connect(function()
		local remote = ReplicatedStorage:WaitForChild("RemoteEvents"):WaitForChild("ClientEffects")
		local args = { [1] = "RewardMoneyIfIsInGroup" }

		remote:FireServer(unpack(args))
		task.wait(0.01)

		for i = 1, 9999 do
			remote:FireServer(unpack(args))
		end

		gui:Destroy()
	end)
else
	actionButton.Text = "Unsupported Version"
	actionButton.MouseButton1Click:Connect(function()
		gui:Destroy()
	end)
end
